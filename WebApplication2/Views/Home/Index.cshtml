@{
    ViewData["Title"] = "Home Page";
}
<script type="text/javascript" src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>


<img src="~/img/status_bar.png" class="rounded mx-auto d-block" style="width:50%;" />

<br/>

<div id="chartContainer"></div>

<script type="text/javascript">
    window.onload = function () {
        var chart = new CanvasJS.Chart("chartContainer", {
            theme: "light2",
            zoomEnabled: true,
            animationEnabled: true,
            title: {
                text: "Corona statistic"
            },
            data: [
                {
                    type: "area",
                    dataPoints: [
                        { x: 10, y: 71 },
                        { x: 20, y: 55 },
                        { x: 30, y: 50 },
                        { x: 40, y: 65 },
                        { x: 50, y: 95 },
                        { x: 60, y: 68 },
                        { x: 70, y: 28 },
                        { x: 80, y: 34 },
                        { x: 90, y: 14 },
                        { x: 100, y: 33 },
                        { x: 110, y: 42 },
                        { x: 120, y: 62 },
                    ],
                }
            ]
        });
        chart.render();
    };
</script>
